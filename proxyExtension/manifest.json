
    {
        "version": "1.0.0",
        "manifest_version": 2,
        "name": "Đổi proxy qua url",
        "permissions": [
            "proxy",
            "tabs",
            "unlimitedStorage",
            "storage",
            "<all_urls>",
            "webRequest",
            "webRequestBlocking"
        ],
        "background": {
            "scripts": ["background.js"]
        },
        "icons": {
            "16": "icon/16.png",
            "32": "icon/32.png",
            "48": "icon/48.png",
            "128": "icon/128.png"
        },
        "minimum_chrome_version":"22.0.0"
    }
    